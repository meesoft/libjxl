      "buildRoot": "${projectDir}\\out\\build\\${name}",
      "installRoot": "${projectDir}\\out\\install\\${name}",
      "cmakeCommandArgs": "-DCMAKE_TOOLCHAIN_FILE=D:\\PROGRM\\Git\\vcpkg/scripts/buildsystems/vcpkg.cmake",
